{
    "name": "G Dark (Vulcan)",
    "type": "dark",
    "tokenColors": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment",
          "string.quoted.docstring"
        ],
        "settings": {
          // "fontStyle": "italic bold",
          // alternate base color for bg : #1b1b21
          "foreground": "#4f5553"
        }
      },
      {
        "name": "Docstring",
        "scope": [
          "punctuation.definition.comment",
          "string.quoted.docstring"
        ],
        "settings": {
          "fontStyle": "italic bold",
        }
      },
      {
        "name": "Variables",
        "scope": [
          "variable",
          "string constant.other.placeholder"
        ],
        "settings": {
          "foreground": "#91beb8d5",
          "fontStyle": "bold"
        }
      },
      {
        "name": "PHP Constants",
        "scope": [
          "constant.other.php"
        ],
        "settings": {
          "foreground": "#e47e0a"
        }
      },
      {
        "name": "Colors",
        "scope": [
          "constant.other.color"
        ],
        "settings": {
          "foreground": "#8d4d03",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#FF5370"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": [
          "invalid.deprecated"
        ],
        "settings": {
          "foreground": "#C792EA"
        }
      },
      {
        "name": "Keyword",
        "scope": [
          "keyword",
        ],
        "settings": {
          "foreground" : "#c97b56"
        }
      },
      {
        "name": "Storage",
        "scope": [
          "storage.type",
        ],
        "settings": {
          "foreground" : "#63D4E7"
        }
      },
      {
        "name": "Modifier",
        "scope": [
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#439CD8",
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "Keyword",
          "Storage"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Operator, Punctuation",
        "scope": [
          "punctuation",
          "punctuation.definition.tag",
          "punctuation.section.embedded",
        ],
        "settings": {
          "foreground": "#DC9DA8"
        }
      },
      {
        "name": "php dollar sign",
        "scope": "punctuation.definition.variable.php",
        "settings": {
          "foreground": "#1a8879"
        }
      },
      {
        "name": "Operator, Keyword",
        "scope": [
          "keyword.control",
          "constant.other.color",
          "keyword.other.template",
          "keyword.other.substitution",
          "punctuation.separator.inheritance.php",
        ],
        "settings": {
          "foreground": "#4ea064"
        }
      },
      {
        "name": "Keyword Control",
        "scope": [
          "keyword.control"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter",
          "punctuation.definition.tag.html",
          // "punctuation.definition.tag.begin.html",
          // "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#337BB0"
        }
      },
      {
        "name": "Open-End Tag",
        "scope": [
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#439CD8",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Function",
        "scope": [
          "meta.function-call",
          "variable.function",
          "support.function",
          "keyword.other.special-method"
        ],
        "settings": {
          "foreground": "#be7587"
        }
      },
      {
              "name": "Special Method, Block Level, GitGutter changed",
              "scope": [
                  "entity.name.function",
              ],
              "settings": {
                  "fontStyle": "",
                  "foreground": "#9E4496"
              }
      },
      {
        "name": "C-related Block Level Variables",
        "scope": [
          "source.cpp meta.block variable.other"
        ],
        "settings": {
          "foreground": "#91beb8d5"
        }
      },
      {
        "name": "Variables constant",
        "scope": [
          "variable.other.constant"
        ],
        "settings": {
          "foreground": "#91beb8d5"
        }
      },
      {
        "name": "Other Variable, String Link",
