{
    "name": "G Dark-minimal-2 (Astronaut Blue)",
    "type": "dark",
    "tokenColors": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment",
          "string.quoted.docstring"
        ],
        "settings": {
          // "fontStyle": "italic bold",
          "foreground": "#151515"
        }
      },
      {
        "name": "Docstring",
        "scope": [
          "punctuation.definition.comment",
          "string.quoted.docstring"
        ],
        "settings": {
          "fontStyle": "italic bold",
        }
      },
      {
        "name": "Variables",
        "scope": [
          "variable",
          "string constant.other.placeholder"
        ],
        "settings": {
          "foreground": "#cacde1",
          "fontStyle": "bold"
        }
      },
      {
        "name": "PHP Constants",
        "scope": [
          "constant.other.php"
        ],
        "settings": {
          "foreground": "#8e9df3"
        }
      },
      {
        "name": "Language Constants, Constants",
        "scope": [
          "constant.language",
          "support.constant",
          "constant.other.color",
        ],
        "settings": {
          "foreground": "#a3c3f5",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#fcafbc"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": [
          "invalid.deprecated"
        ],
        "settings": {
          "foreground": "#f86898"
        }
      },
      {
        "name": "Keyword",
        "scope": [
          "keyword",
        ],
        "settings": {
          "foreground" : "#71bbfc"
        }
      },
      {
        "name": "Storage",
        "scope": [
          "storage.type",
        ],
        "settings": {
          "foreground" : "#c19dfa"
        }
      },
      {
        "name": "Modifier",
        "scope": [
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#BBFFBB",
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "Keyword",
          "Storage"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Operator, Punctution",
        "scope": [
          "punctuation",
          "punctuation.definition.tag",
          "punctuation.section.embedded",
        ],
        "settings": {
          "foreground": "#96ceb4"
        }
      },
      {
        "name": "php dollar sign",
        "scope": [
          "punctuation.definition.variable.php",
          "punctuation.definition.varaible.less"
        ],
        "settings": {
          "foreground": "#7084F1"
        }
      },
      {
        "name": "Operator, Keyword",
        "scope": [
          "keyword.control",
          "constant.other.color",
          "keyword.other.template",
          "keyword.other.substitution",
          "punctuation.separator.inheritance.php",
        ],
        "settings": {
          "foreground": "#90b7fa"
        }
      },
      {
        "name": "Keyword Control",
        "scope": [
          "keyword.control"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter",
          "punctuation.definition.tag.html",
          // "punctuation.definition.tag.begin.html",
          // "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#9eaef3"
        }
      },
      {
        "name": "Open-End Tag",
        "scope": [
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#6d7ddd",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Function",
        "scope": [
          "meta.function-call",
          "variable.function",
          "support.function",
          "keyword.other.special-method"
        ],
        "settings": {
          "foreground": "#6ea8ff"
        }
      },
      {
        "name": "Special Method, Block Level, GitGutter changed",
        "scope": [
            "entity.name.function",
        ],
        "settings": {
            "fontStyle": "bold",
            "foreground": "#2ecde2"
        }
      },
      {
        "name": "C-related Block Level Variables",
        "scope": [
          "source.cpp meta.block variable.other"
