{
  "name": "G Dark-minimal-2 (Astronaut Blue)",
  "type": "dark",
  "tokenColors": [{
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        // "fontStyle": "italic bold",
        "foreground": "#172628"
      }
    },
    {
      "name": "Docstring",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#a5ddf1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants, Variables constant",
      "scope": [
        "constant.other.php",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#8497ff",
        "fontStyle": ""
      }
    },
    {
      "name": "Language Constants, Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.other.color",
      ],
      "settings": {
        "foreground": "#88afee",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#fcafbc"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#f86898"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground": "#71bbfc"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground": "#b080ff"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#b3f6b3",
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#98d7ba"
      }
    },
    {
      "name": "php dollar sign",
      "scope": [
        "punctuation.definition.variable.php",
        "punctuation.definition.variable.less"
      ],
      "settings": {
        "foreground": "#90a0f9"
      }
    },
    {
      "name": "Operator, Keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.separator.inheritance.php",
      ],
      "settings": {
        "foreground": "#93bbff"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#99acff"
      }
    },
    {
      "name": "Open-End Tag",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#6d7dd4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": [
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#82cbff"
      }
    },
    {
      "name": "Special Method, Block Level, GitGutter changed",
      "scope": [
        "entity.name.function",
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#48ccea"
      }
    },
    {
      "name": "C-related Block Level Variables",
      "scope": [
        "source.cpp meta.block variable.other"
