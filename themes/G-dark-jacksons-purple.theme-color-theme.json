{
  "name": "G Dark (Jacksons Purple)",
  "type": "dark",
  "tokenColors": [{
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        // "fontStyle": "italic bold",
        "foreground": "#101010bb"
      }
    },
    {
      "name": "Docstring",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#b0caf0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants, Variables Constant",
      "scope": [
        "constant.other.php",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#a7d251",
        "fontStyle": ""
      }
    },
    {
      "name": "Language, Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#d8bf90",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground": "#ff9abd"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground": "#ff7eb4"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d99fff",
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#96ceb4"
      }
    },
    {
      "name": "php dollar sign",
      "scope": [
        "punctuation.definition.variable.php",
        "punctuation.definition.variable.less",
        "punctuation.definition.variable.sass",
        "punctuation.definition.variable.scss",
        "punctuation.definition.variable.stylus",
