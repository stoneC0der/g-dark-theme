{
  "name": "G Dark-Shader (Haiti)",
  "type": "light",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        // "fontStyle": "italic bold",
        "foreground": "#614c74"
      }
    },
    {
      "name": "Docstring",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#ad71fc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants, Constants",
      "scope": [
        "constant.other.php",
      ],
      "settings": {
        "foreground": "#cc34f1"
      }
    },
    {
      "name": "PHP Constants, Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#aa17e4"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#eb53ff"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#eb7ae1"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground" : "#6726a8"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground" : "#994ce7"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#8f35e9",
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#bb67f3"
      }
    },
    {
      "name": "php dollar sign",
      "scope": "punctuation.definition.variable.php",
      "settings": {
        "foreground": "#9353d3"
      }
    },
    {
      "name": "Operator, Keyword",
      "scope": [
