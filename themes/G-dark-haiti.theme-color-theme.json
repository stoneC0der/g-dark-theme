      "settings": {
        "foreground": "#9f48f5"
      }
    },
    {
			"name": "Special Method, Block Level",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				// "fontStyle": "",
				"foreground": "#8e4cf8"
			}
    },
    {
      "name": "C-related Block Level Variables",
