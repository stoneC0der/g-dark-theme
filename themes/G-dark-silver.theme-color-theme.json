{
  "name": "G Dark Silver",
  "type": "light",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        // "fontStyle": "italic bold",
        "foreground": "#a6afad9f"
      }
    },
    {
      "name": "Docstring",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#666b6dd7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants",
      "scope": [
        "constant.other.php"
      ],
      "settings": {
        "foreground": "#2c9c0be7"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
      ],
      "settings": {
        "foreground": "#b97e0fe7"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#8d4d03",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground" : "#db6955e1"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground" : "#e920e9e3"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f13d3dea",
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctution",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#d967f3"
      }
    },
    {
      "name": "php dollar sign",
      "scope": "punctuation.definition.variable.php",
      "settings": {
        "foreground": "#1680c7ec"
      }
    },
    {
      "name": "Operator, Keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.separator.inheritance.php",
      ],
      "settings": {
        "foreground": "#fa892ce2"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
