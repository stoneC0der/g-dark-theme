{
    "name": "G Dark-shader (Haiti)",
    "type": "dark",
    "tokenColors": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment",
          "string.quoted.docstring"
        ],
        "settings": {
          // "fontStyle": "italic bold",
          "foreground": "#4e3c5e"
        }
      },
      {
        "name": "Docstring",
        "scope": [
          "punctuation.definition.comment",
          "string.quoted.docstring"
        ],
        "settings": {
          "fontStyle": "italic bold",
        }
      },
      {
        "name": "Variables",
        "scope": [
          "variable",
          "string constant.other.placeholder"
        ],
        "settings": {
          "foreground": "#8b82ac",
          "fontStyle": "bold"
        }
      },
      {
        "name": "PHP Constants",
        "scope": [
          "constant.other.php"
        ],
        "settings": {
          "foreground": "#fac45f"
        }
      },
      {
        "name": "Language Constants, Constants",
        "scope": [
          "constant.language",
          "support.constant",
          "constant.other.color",
        ],
        "settings": {
          "foreground": "#e9ce9c",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Invalid",
        "scope": [
          "invalid",
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#FF5370"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": [
          "invalid.deprecated"
        ],
        "settings": {
          "foreground": "#C792EA"
        }
      },
      {
        "name": "Keyword",
        "scope": [
          "keyword",
        ],
        "settings": {
          "foreground" : "#fa76ad",
        }
      },
      {
        "name": "Storage",
        "scope": [
          "storage.type",
        ],
        "settings": {
          "foreground" : "#fa7385"
        }
      },
      {
        "name": "Modifier",
        "scope": [
          "storage.modifier"
        ],
        "settings": {
          "foreground": "#C792EA",
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "Keyword",
          "Storage"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Operator, Punctuation",
        "scope": [
          "punctuation",
          "punctuation.definition.tag",
          "punctuation.section.embedded",
        ],
        "settings": {
          "foreground": "#96ceb4"
        }
      },
      {
        "name": "php dollar sign",
        "scope": [
          "punctuation.definition.variable.php",
          "punctuation.definition.variable.less"
        ],
        "settings": {
          "foreground": "#fa7a8f"
        }
      },
      {
        "name": "Operator, Keyword",
        "scope": [
          "keyword.control",
          "constant.other.color",
          "keyword.other.template",
          "keyword.other.substitution",
          "punctuation.separator.inheritance.php",
        ],
        "settings": {
          "foreground": "#ff6f69"
        }
      },
      {
        "name": "Keyword Control",
        "scope": [
          "keyword.control"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter",
          "punctuation.definition.tag.html",
          // "punctuation.definition.tag.begin.html",
          // "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#fa7385"
        }
      },
      {
        "name": "Open-End Tag",
        "scope": [
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html"
        ],
        "settings": {
          "foreground": "#f87ab9",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Function",
        "scope": [
          "meta.function-call",
          "variable.function",
          "support.function",
          "keyword.other.special-method"
        ],
        "settings": {
          "foreground": "#6496f1"
        }
      },
      {
              "name": "Special Method, Block Level, GitGutter changed",
              "scope": [
                  "entity.name.function",
              ],
              "settings": {
                  "fontStyle": "bold",
                  "foreground": "#51CEFF"
              }
      },
      {
        "name": "C-related Block Level Variables",
        "scope": [
          "source.cpp meta.block variable.other"
