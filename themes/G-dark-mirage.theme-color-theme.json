{
  "name": "G Dark (Mirage)",
  "type": "light",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        // "fontStyle": "italic bold",
        "foreground": "#3f535c"
      }
    },
    {
      "name": "Docstring",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#7492aad8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants",
      "scope": [
        "constant.other.php"
      ],
      "settings": {
        "foreground": "#3bca0f"
      }
    },
    {
      "name": "Number, Constant",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
      ],
      "settings": {
        "foreground": "#ce8c12e7"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#c96e05",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground" : "#df6c58"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground" : "#dd1cca"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f13d3d",
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctution",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#d967f3"
      }
    },
    {
      "name": "php dollar sign",
      "scope": "punctuation.definition.variable.php",
      "settings": {
        "foreground": "#1989d4"
      }
    },
    {
      "name": "Operator, Keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.separator.inheritance.php",
      ],
      "settings": {
        "foreground": "#e7812d"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "punctuation.definition.tag.html",
      ],
      "settings": {
        "foreground": "#e32a2a"
      }
    },
    {
      "name": "Open-End Tag",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#eb2c82",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": [
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#8237f3"
      }
    },
    {
			"name": "Special Method, Block Level",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				// "fontStyle": "",
				"foreground": "#eb0ea1"
			}
    },
    {
      "name": "C-related Block Level Variables",
      "scope": [
        "source.cpp meta.block variable.other"
      ],
      "settings": {
        "foreground": "#7492aad8"
      }
    },
    {
      "name": "Variables constant",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#7492aad8"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#7492aad8"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "constant.escape",
        "keyword.other.unit",
      ],
      "settings": {
        "foreground": "#c99914"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#0bada0"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
