      "settings": {
        "foreground": "#ea3ebf",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Namespace",
      "scope": [
        "support.other.namespace.php"
      ],
      "settings": {
        "foreground": "#974af0",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Entity Namespace",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "#1275e6",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.class",
      ],
      "settings": {
        "foreground": "#b124e8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type"
