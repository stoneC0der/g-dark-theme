{
  "name": "G Dark (Mirage)",
  "type": "light",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        // "fontStyle": "italic bold",
        "foreground": "#3f535c"
      }
    },
    {
      "name": "Docstring",
      "scope": [
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#c498d6d8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants, Constants",
      "scope": [
        "constant.other.php",
      ],
      "settings": {
        "foreground": "#d418c5"
      }
    },
    {
      "name": "PHP Constants, Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#c70791"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#eb7ae1"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground" : "#e91bd1"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground" : "#f033dd"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f13da0",
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#d967f3"
      }
    },
    {
      "name": "php dollar sign",
      "scope": "punctuation.definition.variable.php",
      "settings": {
        "foreground": "#eb25c3"
      }
    },
    {
      "name": "Operator, Keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.separator.inheritance.php",
      ],
      "settings": {
        "foreground": "#f32bdc"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "punctuation.definition.tag.html",
      ],
      "settings": {
        "foreground": "#e916a6"
      }
    },
    {
      "name": "Open-End Tag",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#bd15a4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": [
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#9f48f5"
      }
    },
    {
			"name": "Special Method, Block Level",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				// "fontStyle": "",
				"foreground": "#ee1da1"
			}
    },
    {
      "name": "C-related Block Level Variables",
      "scope": [
        "source.cpp meta.block variable.other"
