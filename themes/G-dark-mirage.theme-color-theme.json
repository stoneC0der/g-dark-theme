      "settings": {
        "foreground": "#09ad69",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#7aca04"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#f4022b"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
