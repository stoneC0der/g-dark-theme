{
  "name": "G Dark (Midnight)",
  "type": "black",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#284763"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#75b5ce",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants",
      "scope": [
        "constant.other.php"
      ],
      "settings": {
        "foreground": "#a2eb7d"
      }
    },
    {
      "name": "Language, Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#5efab1"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#957dec"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground" : "#0287da",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type",
      ],
      "settings": {
        "foreground" : "#7fe965"
      }
    },
    {
      "name": "Modifier",
      "scope": [
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#3bdf36",
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "Keyword",
        "Storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#2ed67af6"
      }
    },
    {
      "name": "php dollar sign",
      "scope": "punctuation.definition.variable.php",
      "settings": {
        "foreground": "#3aa9e9"
      }
    },
    {
      "name": "Operator, Keyword",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution",
        "punctuation.separator.inheritance.php",
      ],
      "settings": {
        "foreground": "#67a7fa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#2bcf39"
      }
    },

    {
      "name": "Open/Close Tag",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#6fda6c"
      }
    },
    {
      "name": "Function",
      "scope": [
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#5e75f8",
      }
    },
    {
			"name": "Special Method, Block Level, GitGutter changed",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#06cdfa"
			}
    },
    {
      "name": "C-related Block Level Variables",
      "scope": [
        "source.cpp meta.block variable.other"
      ],
      "settings": {
        "foreground": "#75b5ce"
      }
    },
    {
      "name": "Variables constant",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#75b5ce"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": [
        "support.other.variable",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#75b5ce"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
