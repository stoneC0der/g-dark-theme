        "constant.language",
        "support.constant",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#5AD982",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Language methods",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B8A7F1"
      }
    },
    {
