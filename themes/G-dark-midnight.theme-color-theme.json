{
  "name": "G Dark-minimal (Midnight)",
  "type": "black",
  "tokenColors": [{
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6f7c8b"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#79BBD3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "PHP Constants,Variables constant",
      "scope": [
        "constant.other.php",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#97c179",
        "fontStyle": ""
      }
    },
    {
