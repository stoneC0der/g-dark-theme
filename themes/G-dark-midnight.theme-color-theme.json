        "constant.numeric",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#42a4b6"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        // "fontStyle": "normal",
        "foreground": "#47d5a3"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.other.namespace.use.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#4ef3f3",
        "fontStyle": "bold"
      }
    },

    {
      "name": "Class, Support",
      "scope": [
        "support.namespace.php",
        "meta.use.php",
