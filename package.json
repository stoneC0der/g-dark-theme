{
    "name": "g-dark-theme",
    "displayName": "G-dark",
    "description": "Minimalistic Blue Whale vscode theme.",
    "version": "0.0.1",
    "publisher" : "StoneC0der",
    "icon" : "images/g-darklogo.png",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Themes",
        "Other"
    ],

    "repository": {
        "type": "git",
        "url": "https://github.com/stoneC0der/g-dark-theme"
    },
    "keywords": [
        "atom",
        "dark",
        "blue Whale",
        "vscode",
        "material",
        "Midnight"
    ],

    "galleryBanner": {
        "color": "#003248",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "G-dark-default",
                "uiTheme": "vs-dark",
                "path": "./themes/G-dark.theme-color-theme.json"
            },
            {
                "label": "G-dark-light",
                "uiTheme": "vs",
                "path": "./themes/G-dark.theme.light-color-theme.json"
            },
            {
                "label": "G-dark-black",
                "uiTheme": "vs-dark",
                "path": "./themes/G-dark.theme.contrast-theme.json"
            }
        ]
    }
}